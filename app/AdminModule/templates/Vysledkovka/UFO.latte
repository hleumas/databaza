<table>
  <thead>
    <tr>
      <th n:foreach="array('Por.', 'Meno', 'Å kola', 'U1', 'U2', 'U3', 'U4', 'Bonus', 'Sum', 'Body') as $name">{$name}</th>
    </tr>
  </thead>
  <tbody>
    <tr n:foreach="$vysledkovkaA as $riesitel" n:class="$iterator->even ? parity0 : parity1">
